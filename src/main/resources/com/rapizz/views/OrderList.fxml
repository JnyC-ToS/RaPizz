<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.rapizz.controllers.OrderList" spacing="20.0">
	<padding>
		<Insets bottom="35.0" left="35.0" right="35.0" top="35.0" />
	</padding>
	<Label styleClass="title" text="Mes commandes" />
	<GridPane fx:id="ordersContainer" hgap="35.0" vgap="35.0" />
	<Label fx:id="empty" styleClass="notice" text="Aucune commande">
		<VBox.margin>
			<Insets top="-15.0" left="15" />
		</VBox.margin>
	</Label>
</VBox>
